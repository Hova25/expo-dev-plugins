{
  "name": "rapid-debugger",
  "version": "0.0.1",
  "description": "DevTool for Expo to debug speedly an app",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "expo-module build",
    "build:all": "expo-module prepare && cd webui && npx expo export -p web --output-dir ../dist",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare && node ../../scripts/build-webui.js react-query",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "web:dev": "cd webui && yarn start",
    "web:export": "cd webui && yarn build"
  },
  "keywords": [
    "expo",
    "devtools"
  ],
  "files": [
    "build",
    "dist",
    "expo-module.config.json"
  ],
  "license": "MIT",
  "dependencies": {
    "flatted": "^3.3.1"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.7.2",
    "expo": "~50.0.1",
    "expo-module-scripts": "^3.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": "*",
    "expo": "*"
  }
}
